%section
	.container
		.row
			.col-lg-12
				=partial "/shared/subnav_with_links"
		.row.double-margin-bottom
			.col-lg-12
				%h3
					Saved and Incomplete Tests are displayed below.
				%p.no-margin-bottom
					Included below are tests that were saved or interrupted while using the system. Delete or Resume a test by selecting from the list. All responses submitted will be restored upon resuming a test.
		.row
			.col-lg-12
				.label.label-warning.half-margin-right.pull-left{:style=>"position:relative; top:6px;"}
					3 
				%h3.pull-left
					Tests in Progress
		.row
			.col-lg-12
				-3.times do
					=partial "/shared/in-progress-test-thumb-lg", :locals=>{ :status=>"In Progress", :iconClass=>"fa-circle", :btnTitle=>"Resume Test", :textStatusClass=>"text-warning"}

				